{
  "plugins": [
    "react","import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
     "standard-react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "es6": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "jsx-quotes": 0
  }
}
